{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
      "moduleResolution": "Node",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "src",
      "paths": {
        "@components/*": ["components/*"],
        "@assets/*": ["assets/*"],
      }
    },
    "include": ["src"]
  }
  